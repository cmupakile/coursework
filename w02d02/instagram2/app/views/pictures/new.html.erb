<h1>New Picture</h1>


<form action="<%= pictures_path%>" method="POST" enctype="multipart/form-data" >

  <label>Caption</label>
  <textarea name="picture[caption]"></textarea>
</div>

<div class="field">
  <label>Image here</label>
  <input type="file" name="picture[image]">
</div>

<input type="submit" name="save" value="save">
<input name="authenticity_token" value="<%= form_authenticity_token %>" type="hidden">


</form>
